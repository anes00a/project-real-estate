{
  "version": 2,
  "name": "node-js",
  
    "builds": [
      {
        "src": "api/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "client/build" }
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "src": "/(.*)", "dest": "/client/$1" }
    ]
  
  
}